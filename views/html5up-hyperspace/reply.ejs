	<body>

		<!-- Header -->
			<header id="header">
				<a href="/" class="title"><img class="logo" src="images/dounoa-network.jpg"></a>
				<nav>
					<ul>
						<li><a href="/">View all Searches</a></li>
						<li><a href="/post">Post A Search</a></li>
						<li><a href="/my-profile">My Profile</a></li>
						<li><a href="/logout">Logout</a></li>
					</ul>
				</nav>
			</header>

		<!-- Wrapper -->
			<div id="wrapper">

				<!-- Main -->
					<section id="main" class="wrapper">
						<div class="inner">
							<h1 class="major">I am a... <%= search %></h1>
              <form method="post" action="/reply?email=<%= email %>">
                <div>
                  <label for="name">Name of the <%= search %></label>
                  <input type="text" name="name" id="name" />
                </div>
								<div>
                  <label for="email">Email of the <%= search %></label>
                  <input type="text" name="email" id="email" />
                </div>
								<p>Note: This contact information will not be shared with anyone other than <%= email %></p>
                <div>
                  <label for="qualifications">Qualifications/Comments</label>
                  <textarea name="qualifications" id="qualifications" rows="4"></textarea>
                </div>
                <input type="hidden" name="search" value="<%= search %>" />
                <!-- <input type="hidden" name="email" value="<%= email %>" /> -->
                <ul class="actions">
                  <li><a href="#" class="button submit">Send</a></li>
                  <div id="display"></div>
                </ul>
              </form>
						</div>
					</section>

			</div>

		<!-- Footer -->
			<footer id="footer" class="wrapper alt">
